{
    "version": 2,
    "functions": {
        "api/index.js": {
            "includeFiles": "backend/src/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/frontend/dist/assets/$1"
        },
        {
            "source": "/favicon.ico",
            "destination": "/frontend/dist/favicon.ico"
        },
        {
            "source": "/logo_soissons.jpg",
            "destination": "/frontend/dist/logo_soissons.jpg"
        },
        {
            "source": "/manifest.json",
            "destination": "/frontend/dist/manifest.json"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/frontend/dist/index.html"
        }
    ]
}